{
  "name": "workshop-july-2019",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "antd": "^3.20.6",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.3",
    "apollo-link": "^1.2.12",
    "apollo-link-http": "^1.5.15",
    "apollo-link-ws": "^1.0.18",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "express-session": "^1.16.2",
    "graphql": "^14.4.2",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.5",
    "isomorphic-fetch": "^2.2.1",
    "next": "^9.0.0",
    "node-sass": "^4.12.0",
    "passport": "^0.4.0",
    "passport-github": "^1.1.0",
    "react": "^16.8.6",
    "react-apollo": "^2.5.8",
    "react-dom": "^16.8.6",
    "subscriptions-transport-ws": "^0.9.16"
  },
  "scripts": {
    "dev": "yarn build:types && cross-env BASE_URL=localhost:3000 nodemon",
    "introspect": "apollo client:download-schema --endpoint http://graphql-workshop-2019.herokuapp.com/graphql schema.json",
    "introspect:local": "apollo-codegen introspect-schema schema.graphql --output schema.json",
    "build:types": "apollo client:codegen --localSchemaFile schema.json --target typescript --includes \"{pages,components}/**/*.{tsx,ts}\" --excludes node_modules --outputFlat",
    "build": "yarn build:types && next build && tsc --project tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node dist/index.js"
  },
  "devDependencies": {
    "@types/react": "^16.8.23",
    "apollo": "^2.16.1",
    "apollo-codegen": "^0.20.2",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  }
}
